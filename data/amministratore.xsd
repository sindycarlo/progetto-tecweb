<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
elementFormDefault="qualified" >

<xsd:element name="amministratore">
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element name="login" type="xsd:string"/>
			<xsd:element name="password" type="PWType"/>
		</xsd:sequence>
	</xsd:complexType>
</xsd:element>

<xsd:element name="PWType">
	<xsd:simpleType>
		<xsd:restriction base="xsd:string">
			<xsd:minLenght value="8"/>
			<xsd:maxLenght value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:element>

</xsd:schema>
